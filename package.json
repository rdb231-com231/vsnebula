{
  "name": "vsneb",
  "displayName": "vsneb",
  "description": "Visual Studio Code support for Nebula",
  "publisher": "MewPlush",
  "icon": "./neb.png",
  "extensionDependencies": ["pkief.material-icon-theme"],
  "author": {
    "name": "MewPlush",
    "email": "flaviofdbastos@gmail.com",
    "url": "https://github.com/rdb231-com231/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rdb231-com231/vsnebula"
  },
  "version": "1.4.5",
  "license": "MIT",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "neb",
        "aliases": ["Nebula", "neb"],
        "extensions": [".neb"],
        "configuration": "./language-configuration.json",
        "icon": {
          "dark": "./neb.png",
          "light": "./neb.png"
        }
      },
      {
        "id": "modneb",
        "aliases": ["ModNebula", "modneb"],
        "extensions": [".modneb"],
        "configuration": "./language-configuration.json",
        "icon": {
          "dark": "./modneb.png",
          "light": "./modneb.png"
        }
      }
  ],
    "grammars": [
      {
      "language": "neb",
      "scopeName": "source.nebula",
      "path": "./syntaxes/neb.tmLanguage.json"
      },
      {
      "language": "modneb",
      "scopeName": "source.nebula",
      "path": "./syntaxes/neb.tmLanguage.json"
      }
    ],
    "iconThemes": [{
      "id": "neb",
      "label": "Nebula Icons",
      "path": "./themes/neb.json"
    }]
  }
}
