{
  "name": "vsneb",
  "displayName": "vsneb",
  "description": "Visual Studio Code support for Nebula",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "neb",
      "aliases": ["Nebula", "neb"],
      "extensions": [".neb"],
      "configuration": "./language-configuration.json",
      "icon": {
        "dark": "./neb.png",
        "light": "./neb.png"
      }
    }],
    "grammars": [{
      "language": "neb",
      "scopeName": "source.nebula",
      "path": "./syntaxes/neb.tmLanguage.json"
    }]
  }
}
